import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner (System.in);
        
        double X = sc.nextFloat();
        
        if(X<=0){
            X=X*(-1);
        }
        else if(X <=0 || X<=10){
            X=Math.pow(X,2);
        }else{
            X=Math.sqrt(X);
            
        }
        System.out.printf("%.2f\n", X);
    }
}